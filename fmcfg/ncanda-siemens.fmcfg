scanner=SIEMENS 

unwarpdir="y"
signallossthresh=0.9 # Areas with 90% or more signal loss in fieldmap are masked out
#Used by fugue to unwarp EPI (secs)
dwelltime=0.000425

# difference of gre magnitude TEs is used to calculate rad/s of fieldmap (secs)
TEdiff=0.00246

# TE of the epi is used by sigloss and expected in seconds
epiTE=0.002
